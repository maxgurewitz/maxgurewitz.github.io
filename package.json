{
  "name": "personal-site-typescript",
  "version": "1.0.0",
  "description": "Personal site implemented in typescript.",
  "main": "index.js",
  "scripts": {
    "disc": "browserify --full-paths -p [ tsify --noImplicitAny ] -t [ uglifyify --global ] --debug dev.ts | discify --open",
    "generate-site": "node bin/generate-site",
    "start": "budo dev.ts --dir assets --live --open --host localhost -- -p [ tsify --noImplicitAny ] --debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxgurewitz/personal-site-typescript.git"
  },
  "keywords": [
    "personal",
    "site",
    "typescript",
    "redux",
    "react"
  ],
  "author": "Maxwell Gurewitz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxgurewitz/personal-site-typescript/issues"
  },
  "homepage": "https://github.com/maxgurewitz/personal-site-typescript#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.30",
    "@types/react": "^0.14.29",
    "@types/react-dom": "^0.14.14",
    "@types/react-redux": "^4.4.29",
    "@types/rebass": "^0.2.15",
    "budo": "^8.3.0",
    "concat-stream": "^1.5.1",
    "disc": "^1.3.2",
    "envify": "^3.4.1",
    "lodash-es": "^4.14.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "rebass": "^0.3.1",
    "redux": "^3.5.2",
    "tsify": "^1.0.3",
    "typescript": "^2.1.0-dev.20160809",
    "uglifyify": "^3.0.2"
  }
}
